Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"EqualTransitionCount", "::", "usage"}], "=", 
   "\"\<Finds out the quantity of shared transitions between two k-ary rule \
tables.\>\""}], ";"}], "\n", 
 RowBox[{
  RowBox[{
   RowBox[{"EqualTransitionCount", "[", 
    RowBox[{"karyRuleTableA_", ",", "karyRuleTableB_"}], "]"}], ":=", 
   RowBox[{"Plus", "@@", 
    RowBox[{"MapThread", "[", 
     RowBox[{
      RowBox[{
       RowBox[{"If", "[", 
        RowBox[{
         RowBox[{"#1", "\[Equal]", "#2"}], ",", "1", ",", "0"}], "]"}], "&"}],
       ",", 
      RowBox[{"{", 
       RowBox[{"karyRuleTableA", ",", "karyRuleTableB"}], "}"}]}], "]"}]}]}], 
  "\[IndentingNewLine]"}], "\n", 
 RowBox[{
  RowBox[{
   RowBox[{"SymmetryValues", "::", "usage"}], "=", 
   "\"\<Finds the internal symmetry values according to one of the \
transformations.\>\""}], ";"}], "\n", 
 RowBox[{
  RowBox[{
   RowBox[{"SymmetryValues", "[", 
    RowBox[{"transformation_", ",", 
     RowBox[{"initRuleTables_List:", " ", 
      RowBox[{"{", "}"}]}]}], "]"}], ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
      "transformedRuleTables", ",", "both", ",", "bothValues", ",", 
       "ruleTables"}], "}"}], ",", 
     RowBox[{
      RowBox[{"ruleTables", "=", 
       RowBox[{"If", "[", 
        RowBox[{
         RowBox[{
          RowBox[{"Length", "[", "initRuleTables", "]"}], "\[Equal]", "0"}], 
         ",", 
         RowBox[{"Tuples", "[", 
          RowBox[{
           RowBox[{"{", 
            RowBox[{"0", ",", "1"}], "}"}], ",", "8"}], "]"}], ",", 
         "initRuleTables"}], "]"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"transformedRuleTables", "=", 
       RowBox[{
        RowBox[{
         RowBox[{"KAryFromRuleTable", "[", 
          RowBox[{"transformation", "[", 
           RowBox[{"RuleTableFromKAry", "[", "#", "]"}], "]"}], "]"}], "&"}], 
        "/@", "ruleTables"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"both", "=", 
       RowBox[{"Transpose", "[", 
        RowBox[{"{", 
         RowBox[{"ruleTables", ",", "transformedRuleTables"}], "}"}], "]"}]}],
       ";", "\[IndentingNewLine]", 
      RowBox[{"bothValues", "=", 
       RowBox[{
        RowBox[{
         RowBox[{"{", 
          RowBox[{
           RowBox[{"#", "[", 
            RowBox[{"[", "1", "]"}], "]"}], ",", 
           RowBox[{"#", "[", 
            RowBox[{"[", "2", "]"}], "]"}], ",", 
           RowBox[{"EqualTransitionCount", "[", 
            RowBox[{
             RowBox[{"#", "[", 
              RowBox[{"[", "1", "]"}], "]"}], ",", 
             RowBox[{"#", "[", 
              RowBox[{"[", "2", "]"}], "]"}]}], "]"}]}], "}"}], "&"}], "/@", 
        "both"}]}], ";", "\[IndentingNewLine]", "bothValues"}]}], "]"}]}], 
  "\[IndentingNewLine]"}], "\n", 
 RowBox[{
  RowBox[{
   RowBox[{"RulesWithSymmetry", "::", "usage"}], "=", 
   "\"\<Finds the rules that have a given value for symmetry\>\""}], 
  ";"}], "\n", 
 RowBox[{
  RowBox[{
   RowBox[{"RulesWithSymmetry", "[", 
    RowBox[{"transformations_List", ",", "value_"}], "]"}], ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", "symmetryValues", "}"}], ",", 
     RowBox[{
      RowBox[{"symmetryValues", "=", 
       RowBox[{
        RowBox[{
         RowBox[{
          RowBox[{
           RowBox[{"#", "[", 
            RowBox[{"[", "1", "]"}], "]"}], "&"}], "/@", 
          RowBox[{"Select", "[", 
           RowBox[{
            RowBox[{"SymmetryValues", "[", "#", "]"}], ",", 
            RowBox[{"(", 
             RowBox[{
              RowBox[{
               RowBox[{"Last", "[", "#", "]"}], "\[Equal]", "value"}], "&"}], 
             ")"}]}], "]"}]}], "&"}], "/@", "transformations"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"Apply", "[", 
       RowBox[{"Intersection", ",", "symmetryValues"}], "]"}]}]}], "]"}]}], 
  ";"}], "\n", 
 RowBox[{
  RowBox[{
   RowBox[{"RulesWithSymmetry", "[", 
    RowBox[{"transformation_", ",", "value_"}], "]"}], ":=", 
   RowBox[{
    RowBox[{
     RowBox[{"#", "[", 
      RowBox[{"[", "1", "]"}], "]"}], "&"}], "/@", 
    RowBox[{"Select", "[", 
     RowBox[{
      RowBox[{"SymmetryValues", "[", "transformation", "]"}], ",", 
      RowBox[{
       RowBox[{
        RowBox[{"Last", "[", "#", "]"}], "\[Equal]", "value"}], "&"}]}], 
     "]"}]}]}], ";"}]}], "Input",
